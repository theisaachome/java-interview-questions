package com.isaachome.practice;

import java.util.Scanner;

public class MainPracticeApp {
	static Scanner sc = new Scanner(System.in);
	public static void main(String[] args) {
		int numbers;
		System.out.print("Enter Numbers : ");
		numbers =sc.nextInt();
		int temp = numbers;
		int remainder =0;
		while(temp>0) {
			remainder = temp %10;
		}
	}
}
