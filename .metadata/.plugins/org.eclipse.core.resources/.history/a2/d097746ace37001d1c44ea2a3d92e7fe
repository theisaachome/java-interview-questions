package com.isaachome.numberprogram;

public class ReverseNumberProgram {
	public static void main(String[] args) {
		inverseNumber(0);
	}
	static void inverseNumber(int inputNums) {
		int reverse =0;
		
		while(inputNums >0) {
			// get the last digit from inputNums
			temp=inputNums%10;
			reverse = reverse*10 +
			// delete the last digit
			inputNums/=10;
		}
	}
}
